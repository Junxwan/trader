<Window x:Class="trader.Page.OP5minK"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:trader"
        mc:Ignorable="d"
        xmlns:op="clr-namespace:trader.OPS"
        Title="OP 5分K" WindowState="Maximized" WindowStartupLocation="CenterScreen" x:Name="thisWin" >

    <op:_5minKView 
        Manage="{Binding Path=OP, ElementName=thisWin}" 
        Transaction="{Binding Path=Transaction, ElementName=thisWin}"
        Futures="{Binding Path=Futures, ElementName=thisWin}"/>
</Window>
